<%= flash[:error] if flash[:error] %>

<%= form_for @task do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<%= f.label :test %>
	<%= f.text_field :test %>
	<%= f.label :test_command %>
	<%= f.text_field :test_command %>

	<%= f.submit %>
<% end %>